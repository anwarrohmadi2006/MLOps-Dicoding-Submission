{
  "title": "anwarrohmadi111",
  "uid": "model-monitoring",
  "timezone": "browser",
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (RPS)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(model_request_total[1m])",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "id": 2,
      "title": "Error Rate (%)",
      "type": "gauge",
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "100 * ((sum(rate(model_error_total[5m])) or vector(0)) / clamp_min(sum(rate(model_request_total[5m])), 1e-9))"
        }
      ]
    },
    {
      "id": 3,
      "title": "Latency P50",
      "type": "timeseries",
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(model_request_latency_seconds_bucket[5m]))"
        }
      ]
    },
    {
      "id": 4,
      "title": "Latency P95",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(model_request_latency_seconds_bucket[5m]))"
        }
      ]
    },
    {
      "id": 5,
      "title": "Latency P99",
      "type": "timeseries",
      "gridPos": {
        "x": 8,
        "y": 6,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(model_request_latency_seconds_bucket[5m]))"
        }
      ]
    },
    {
      "id": 6,
      "title": "CPU Usage (Host)",
      "type": "timeseries",
      "gridPos": {
        "x": 16,
        "y": 6,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{id=\"/\"}[5m]) * 100",
          "legendFormat": "CPU %"
        }
      ]
    },
    {
      "id": 7,
      "title": "Memory Usage (Host)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "targets": [
        {
          "expr": "container_memory_working_set_bytes{id=\"/\"}",
          "legendFormat": "Memory"
        }
      ]
    },
    {
      "id": 8,
      "title": "Network RX",
      "type": "timeseries",
      "gridPos": {
        "x": 8,
        "y": 12,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        }
      },
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{id=\"/\"}[1m])",
          "legendFormat": "RX"
        }
      ]
    },
    {
      "id": 9,
      "title": "Network TX",
      "type": "timeseries",
      "gridPos": {
        "x": 16,
        "y": 12,
        "w": 8,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        }
      },
      "targets": [
        {
          "expr": "rate(container_network_transmit_bytes_total{id=\"/\"}[1m])",
          "legendFormat": "TX"
        }
      ]
    },
    {
      "id": 10,
      "title": "Prediction Mean",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 18,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      },
      "targets": [
        {
          "expr": "model_prediction_mean"
        }
      ]
    },
    {
      "id": 11,
      "title": "Prediction Std",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 18,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      },
      "targets": [
        {
          "expr": "model_prediction_std"
        }
      ]
    },
    {
      "id": 12,
      "title": "Active Requests",
      "type": "stat",
      "gridPos": {
        "x": 12,
        "y": 18,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "model_active_requests"
        }
      ]
    }
  ],
  "schemaVersion": 38,
  "version": 2
}